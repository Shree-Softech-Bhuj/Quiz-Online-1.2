<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorPrimaryDark" />
            <stroke
                android:width="2px"
                 />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">

        <shape android:shape="oval">
            <solid android:color="@color/white" />

        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">

        <shape android:shape="oval">

            <solid android:color="@color/color_orange" />

        </shape>
    </item>
</layer-list>